{
  "name": "ember-apollo-server",
  "version": "0.2.0",
  "description": "Apollo Server inside Ember CLI.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Paul Chavard",
  "directories": {
    "doc": "doc",
    "test": "tests"
